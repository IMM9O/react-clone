{
  "name": "MediumTab",
  "description": "Medium articles, the good way!",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["management", "history", "storage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
